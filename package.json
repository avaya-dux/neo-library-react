{
  "name": "neo-library-react",
  "version": "0.1.0",
  "description": "Made with create-react-library",
  "author": "joe-s-avaya",
  "license": "MIT",
  "repository": "neo-library-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "rollup -c -w",
    "build": "rollup -c --environment BUILD:production",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage=true --watchAll=false",
    "test:watch": "react-scripts test",
    "test:cypress": "cypress run-ct",
    "test:cypress-open": "cypress open-ct",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook",
    "github": "yarn && yarn lint && yarn build && yarn test",
    "prepublish": "yarn lint && yarn build && yarn test && yarn test:cypress"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*Types.ts",
      "!src/**/index.ts",
      "!src/**/*.stories.{jsx,tsx}",
      "!src/**/*.cypresstest.tsx"
    ]
  },
  "peerDependencies": {
    "@avaya/neo": "^3.42.0",
    "react": "^17.0.2",
    "ts-essentials": "^9.0.0"
  },
  "devDependencies": {
    "@avaya/neo": "^3.42.0",
    "@babel/core": "^7.15.5",
    "@cypress/react": "^5.10.1",
    "@cypress/webpack-dev-server": "^1.6.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.9",
    "@storybook/testing-react": "^0.0.22",
    "@testing-library/dom": "^8.7.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^8.5.0",
    "cypress-real-events": "^1.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest-axe": "^5.0.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.58.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-smart-asset": "^2.1.2",
    "rollup-plugin-terser": "^7.0.2",
