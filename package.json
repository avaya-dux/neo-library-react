{
  "name": "@avaya/neo-react",
  "version": "0.1.0",
  "description": "This is the React version of the shared library called 'NEO' buit by Avaya",
  "author": "joe-s-avaya",
  "license": "Copyright 2020-2021 Avaya Inc. All Rights Reserved.",
  "repository": "neo-library-react",
  "main": "dist/avaya-neo-react.cjs.js",
  "module": "dist/avaya-neo-react.esm.js",
  "browser": "dist/avaya-neo-react.umd.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "jsdocs": "jsdoc -c .jsdoc.json",
    "jsdocs:verbose": "jsdoc -c .jsdoc.json --verbose",
    "lint": "eslint .",
    "start": "rollup -c -w",
    "build": "yarn build:src && yarn build:types",
    "build:src": "rollup -c --environment BUILD:production",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "clean": "rm -rf coverage dist jsdocs storybook-static",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage=true --watchAll=false",
    "test:watch": "react-scripts test",
    "test:cypress": "cypress run-ct",
    "test:cypress-open": "cypress open-ct",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook",
    "github": "yarn && yarn lint && yarn build && yarn test",
    "all": "yarn && yarn clean && yarn lint && yarn build && yarn test:coverage && yarn test:cypress && yarn build-storybook && yarn jsdocs"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*Types.ts",
      "!src/**/index.ts",
      "!src/**/*.stories.{jsx,tsx}",
      "!src/**/*.cypresstest.tsx"
    ]
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react-table": "^7.7.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@avaya/neo": "^3.48.1",
    "loglevel": "^1.7.1",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "ts-essentials": "^9.0.0"
  },
  "devDependencies": {
    "@avaya/neo": "^3.48.1",
    "@babel/core": "^7.16.0",
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.8",
    "@storybook/testing-react": "^1.2.2",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-table": "^7.7.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "better-docs": "^2.3.2",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.1",
    "cypress-real-events": "^1.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest-axe": "^5.0.1",
    "jsdoc": "^3.6.7",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.60.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.2"
  }
}
